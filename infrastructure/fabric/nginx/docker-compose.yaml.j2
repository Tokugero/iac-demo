  nginx:
    image: nginx
    ports:
    - 80:80
    - 443:443
    volumes:
    - {{ persistent_path }}/nginx/nginx.conf:/etc/nginx/nginx.conf:ro
    - {{ persistent_path }}/nginx/cert.pem:/etc/nginx/cert.pem:ro
    - {{ persistent_path }}/nginx/cert.key:/etc/nginx/cert.key:ro
    restart: unless-stopped
